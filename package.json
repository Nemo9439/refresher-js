{
  "name": "refresher",
  "version": "1.0.0",
  "description": "Detects on client side that a new version of the application is available and notifying the active users to refresh their page",
  "main": "refresher.js",
  "scripts": {
    "test": "test",
    "build": "uglifyjs --compress --mangle -- refresher.js > dist/refresher.latest.min.js",
    "start": "http-server -p 8081",
    "angular-demo": "http-server -p 8080 demo/angular-demo/dist/angular-demo -o",
    "angular-demo-custom-callback": "http-server -p 8080 demo/angular-demo-with-custom-callback/dist/angular-demo-with-custom-callback -o"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nemo9439/refresher-js.git"
  },
  "keywords": [
    "version",
    "update",
    "notification",
    "client",
    "angular",
    "web-application"
  ],
